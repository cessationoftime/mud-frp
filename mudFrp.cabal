name: mudFrp
version: 1.0.0.2
cabal-version: >=1.6
build-type: Simple
license: BSD3
license-file: LICENSE
author: Chris Van Vranken
data-files: *.png
data-dir: data

flag threaded
    Default: False

executable mudFrp
    build-depends: executable-path >=0.0.3 && <0.1,
                   filepath >=1.3.0.1 && <1.4, wx >=0.90.1.0 && <0.91,
                   wxcore >=0.90.1.1 && <0.91, reactive-banana >=0.8.0.0 && <0.9,
                   reactive-banana-wx >=0.8.0.0 && <0.9, random >=1.0.1.1 && <1.1,
                   base >=4.2 && <5

    if flag(threaded)
        buildable: True
        ghc-options: -threaded
    main-is: hello.hs
    buildable: True
    hs-source-dirs: src
    other-modules: Paths_mudFrp Paths

